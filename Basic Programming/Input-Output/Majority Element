int majorityElement(int arr[], int n) {
    int num;
    
    for(int i=0 ; i<n ; i++){
      int count=0;
      for(int j=0 ; j<n ; j++){
          if(arr[j]==arr[i]){
              count++;
              num=arr[i];
          }
          continue;
      }
      if(count>n/2){
          return num;
      }
      continue;
      
    }
    return -1;
}
