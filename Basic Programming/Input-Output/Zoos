#include<bits/stdc++.h>
using namespace std;

void Is_zoo(string s)
{
    int count_z=0,count_o=0;
    if(s.length()<=20)
    {
        for(int i=0 ; i<s.length() ; i++)
        {
            if(s[i]=='z')
            count_z++;
            if(s[i]=='o')
            count_o++;
        }
        if(2*count_z==count_o)
        cout<<"Yes";
        else
        cout<<"No";
    }
}

int main()
{
    string str;
    cin>>str;
    Is_zoo(str);
    return 0;
}

