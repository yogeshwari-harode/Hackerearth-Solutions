#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n; cin>>n;
    vector<int>arr(n);

    for(int i=0 ; i<n ; i++)
        cin>>arr[i];

    int value; cin>>value;

    for(int i=0 ; i<n ; i++)
    {
        if(arr[i]==value)
        {
            cout<<i;
        }

    }
    return 0;
}
